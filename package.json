{
  "name": "babel-plugin-transform-h-jsx",
  "version": "1.0.0",
  "description": "Turns JSX into virtual-hyperscript",
  "repository": "git@github.com:jchook/babel-plugin-transform-h-jsx.git",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "babel",
    "h",
    "jsx",
    "virtual-hyperscript"
  ],
  "jshintConfig": {
    "esversion": 6
  },
  "babel": {
    "ignore": [
      "packages/babel-cli/src/babel-plugin/templates"
    ],
    "stage": 0,
    "loose": [
      "all"
    ],
    "plugins": [
      "flow-comments"
    ],
    "blacklist": [
      "flow",
      "es6.tailCall"
    ],
    "optional": [
      "optimisation.flow.forOf",
      "runtime"
    ],
    "env": {
      "test": {
        "auxiliaryCommentBefore": "istanbul ignore next"
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        2,
        "double",
        "avoid-escape"
      ],
      "no-var": 0,
      "space-after-keywords": 2,
      "strict": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "no-multi-spaces": 0,
      "key-spacing": 0,
      "no-return-assign": 0,
      "consistent-return": 0,
      "no-shadow": 0,
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "no-empty": 0,
      "new-parens": 0,
      "no-cond-assign": 0,
      "no-fallthrough": 0,
      "new-cap": 0,
      "no-loop-func": 0,
      "no-unreachable": 0,
      "no-labels": 0,
      "no-process-exit": 0,
      "camelcase": 0,
      "no-console": 0,
      "no-constant-condition": 0,
      "no-inner-declarations": 0
    }
  },
  "author": "Wes Roberts",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-runtime": "^5.0.0",
    "babel-types": "^6.4.3"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "babel-eslint": "^4.0.6",
    "babel-plugin-flow-comments": "^1.0.9",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.0.0"
  }
}
